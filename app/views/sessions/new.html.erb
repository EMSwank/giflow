<h1>Log into your account</h1>

<%= form_tag login_path do %>
  <%= label_tag :email %>
  <%= text_field_tag :email %>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
  <%= submit_tag "Log in" %>
<% end %>

<h6>Need an account?</h6>
<%= button_to "Sign up!", new_user_path, method: :get %>

